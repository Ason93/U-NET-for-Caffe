  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1024
    pad: 1
    kernel_size: 3
    weight_filler{
      type:"msra"
      std:0.01
    }
  }
}
layer{
  name:"relu9"
  type:"ReLU"
  bottom:"conv5_1"
  top:"conv5_1"
}
layer {
  name: "conv5_2"
  type: "Convolution"
  bottom: "conv5_1"
